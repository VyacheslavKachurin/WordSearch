        </engine:VisualElement>
        <engine:VisualElement name="settings-plate" class="plate plate-show-off" style="flex-grow: 0; align-items: stretch; justify-content: center; display: none;">
            <engine:VisualElement name="row" style="flex-grow: 0; flex-direction: row; padding-left: 103px; padding-right: 187px; align-items: center; justify-content: space-between; margin-bottom: 30px;">
                <engine:VisualElement name="icon" class="icon" style="flex-grow: 0;" />
                <engine:VisualElement name="lbl-div" class="lbl-div" style="flex-grow: 1;">
                    <engine:Label text="Music" class="lbl" />
                </engine:VisualElement>
                <engine:VisualElement name="toggle-div" style="flex-grow: 0;">
                    <engine:Button name="music-btn" class="switch switch-on" />
                </engine:VisualElement>
            </engine:VisualElement>
            <engine:VisualElement name="row" style="flex-grow: 0; flex-direction: row; padding-left: 103px; padding-right: 187px; align-items: center; justify-content: space-between;">
                <engine:VisualElement name="icon" class="icon" style="flex-grow: 0; background-image: url(&quot;project://database/Assets/Sprites/Sounds-icon.png?fileID=2800000&amp;guid=9cb5fa3d17d8d4944bf62a61f8591036&amp;type=3#Sounds-icon&quot;);" />
                <engine:VisualElement name="lbl-div" class="lbl-div" style="flex-grow: 1;">
                    <engine:Label text="Sounds" class="lbl" />
                </engine:VisualElement>
                <engine:VisualElement name="toggle-div" style="flex-grow: 0;">
                    <engine:Button name="music-btn" class="switch switch-on" />
                </engine:VisualElement>
            </engine:VisualElement>
