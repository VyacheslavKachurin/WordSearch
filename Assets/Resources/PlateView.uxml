            <engine:Label text="GET COINS FOR FREE" name="title" style="font-size: 50px; -unity-font-style: bold;" />
            <engine:VisualElement style="flex-grow: 0; flex-direction: row; justify-content: center; align-items: center;">
                <engine:Label text="+50" name="coins-offer-lbl" style="font-size: 50px; -unity-font-style: bold;" />
                <engine:VisualElement name="coin" style="flex-grow: 0; background-image: url(&quot;project://database/Assets/Sprites/coin-big.png?fileID=2800000&amp;guid=35d1d1e52133544ccbac3d76c6cca714&amp;type=3#coin-big&quot;); flex-shrink: 0; width: 87px; height: 92px; margin-left: 16px;" />
            </engine:VisualElement>
            <engine:Button name="watch-ad-btn" text="WATCH VIDEO" icon-image="project://database/Assets/Sprites/ad-icon.png?fileID=2800000&amp;guid=611019146661f4351a59a304d99b0a23&amp;type=3#ad-icon" style="background-image: url(&quot;project://database/Assets/Sprites/ad-btn.png?fileID=2800000&amp;guid=b4aa0edaf6dc245c7876a03656ce14e4&amp;type=3#ad-btn&quot;); width: 567px; height: 125px; font-size: 44px; -unity-font-style: bold; color: rgb(255, 255, 255); justify-content: space-evenly; margin-top: 20px;" />
